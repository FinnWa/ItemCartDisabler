<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="ProductDataImporter\Product\ProductImporter" >
            <argument type="service" id="product.repository"/>
            <argument type="service" id="ProductDataImporter\Product\ProductCollection"/>
            <argument type="service" id="ProductDataImporter\Product\ProductImageToMedia"/>
            <argument type="service" id="ProductDataImporter\Product\ProductValidator"/>
        </service>

        <service id="ProductDataImporter\Product\InputParser" >
            <argument type="service" id="serializer"/>
            <argument type="service" id="ProductDataImporter\Product\ProductImageToMedia"/>
            <argument type="service" id="ProductDataImporter\Product\ProductImageDownloader"/>
        </service>

        <service id="ProductDataImporter\Product\Product" >
        </service>

        <service id="ProductDataImporter\Product\ProductCollection" >
        </service>

        <service id="ProductDataImporter\Product\Command\ProductImportCommand">
            <tag name="console.command"/>
            <argument type="service" id="ProductDataImporter\Product\InputParser"/>
            <argument type="service" id="ProductDataImporter\Product\ProductImporter"/>
        </service>


        <service id="ProductDataImporter\Product\ProductImageDownloader">
            <argument type="service" id="Shopware\Core\Content\Media\MediaService"/>
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver"/>
        </service>

        <service id="ProductDataImporter\Product\ProductMedia">
        </service>

        <service id="ProductDataImporter\Product\ProductMediaCollection">
        </service>

        <service id="ProductDataImporter\Product\ProductMediaImporter">
            <argument type="service" id="product_media.repository"/>
        </service>

        <service id="ProductDataImporter\Product\ProductImageToMedia">
            <argument type="service" id="ProductDataImporter\Product\ProductImageDownloader"/>
            <argument type="service" id="Shopware\Core\Content\Media\File\FileSaver"/>
            <argument type="service" id="Shopware\Core\Content\Media\MediaService"/>
            <argument type="service" id="ProductDataImporter\Product\ProductMediaImporter"/>
        </service>

        <service id="ProductDataImporter\Product\ProductImage">
        </service>

        <service id="ProductDataImporter\Product\ProductValidator">
            <argument type="service" id="serializer"/>
        </service>

    </services>
</container>
